<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/home_title_bar_height"
    android:background="#333333"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_edittitle"
        android:layout_width="match_parent"
        android:layout_height="@dimen/home_title_bar_height"
        android:gravity="center_vertical">

        <RelativeLayout
            android:id="@+id/ThemeTitleLeftLy"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:onClick="onClick"
            android:paddingLeft="16dp"
            android:paddingRight="8dp">

            <!--<ImageView-->
            <!--android:id="@+id/ThemeTitleLeft"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_alignParentLeft="true"-->
            <!--android:layout_centerVertical="true"-->
            <!--android:onClick="onClick"-->
            <!--android:scaleType="centerInside"-->
            <!--android:src="@drawable/btn_prev" />-->
            <font.FTextView
                android:id="@+id/ThemeTitleLeft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:text="@string/cancel"
                android:textColor="@drawable/selector_image_editor_top_title_text"
                android:textSize="13dp"
                custom:customFont="@string/font_name_ygt_740" />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/topView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:orientation="horizontal">

            <font.FTextView
                android:id="@+id/ThemeTitleText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/edit_photo"
                android:textColor="#ffffff"
                android:textSize="@dimen/title_size"
                custom:customFont="@string/font_name_ygt_740" />

            <font.FTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:text="("
                android:textColor="#ffffff"
                android:textSize="@dimen/title_size"
                custom:customFont="@string/font_name_ygt_740" />

            <font.FTextView
                android:id="@+id/photo_count_current_index_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="#E36A63"
                android:textSize="@dimen/title_size"
                custom:customFont="@string/font_name_ygt_740" />

            <font.FTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="/"
                android:textColor="#ffffff"
                android:textSize="@dimen/title_size"
                custom:customFont="@string/font_name_ygt_740" />

            <font.FTextView
                android:id="@+id/photo_count_current_total_count_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff"
                android:textSize="@dimen/title_size"
                custom:customFont="@string/font_name_ygt_740" />

            <font.FTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=")"
                android:textColor="#ffffff"
                android:textSize="@dimen/title_size"
                custom:customFont="@string/font_name_ygt_740" />
        </LinearLayout>

        <font.FTextView
            android:id="@+id/ThemebtnTopNext"
            android:layout_width="54dp"
            android:layout_height="48dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="@string/confirm"
            android:textColor="@drawable/selector_image_editor_top_title_text"
            android:textSize="13dp"
            custom:customFont="@string/font_name_ygt_740" />

    </RelativeLayout>


</LinearLayout>


