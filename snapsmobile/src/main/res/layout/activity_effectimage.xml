                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/activity_effectimage_tmb_shady_ly"
                    android:layout_width="52dp"
                    android:layout_height="34dp">

                    <ImageView
                        android:id="@+id/activity_effectimage_tmb_shady_iv"
